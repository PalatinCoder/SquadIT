{
    "description": "SquadIT is a web application that makes managing your sports squad easy",
    "homepage": "http://squadit.jan-sl.de/",
    "license": "MIT",
    "type": "neos-package",
    "require": {
        "typo3/flow": "~3.3.0",
        "typo3/twitter-bootstrap": "^2.2"
    },
    "name": "squadit/webapp",
	"require-dev": {
		"flowpack/behat": "dev-sqlite-truncate"
	},
    "autoload": {
        "psr-0": {
            "SquadIT\\WebApp": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "SquadIT.WebApp"
        }
    },
    "repositories": {
        "behat-helper": {
            "type": "vcs",
            "url": "https://github.com/PalatinCoder/Flowpack.Behat.git"
        }
    }
}
